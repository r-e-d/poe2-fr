  <Name>conversations\07_neketaka_temple_district\07_bs_gaun_temple_ext</Name>
  <NextEntryID>1</NextEntryID>
  <EntryCount>7</EntryCount>
  <Entries>
    <Entry>
      <ID>2</ID>
      <DefaultText>"Venez prier avec nous."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>3</ID>
      <DefaultText>"Ces eaux ont pour réputation de guérir les fidèles..."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>4</ID>
      <DefaultText>"Êtes-vous venu rendre hommage à Gaun ?"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>5</ID>
      <DefaultText>"Vous avez beau porter la marque de Rikuhu, tous les dieux devraient être honorés."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>6</ID>
      <DefaultText>"Gaun, puisses-tu illuminer mon chemin..."</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>7</ID>
      <DefaultText>"Nous serons témoins d'un nouveau départ !"</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>9</ID>
      <DefaultText>"Gaun est au courant de tous vos actes, meurtrier !"</DefaultText>
      <FemaleText />
    </Entry>
  </Entries>
