      <FemaleText />
    </Entry>
    <Entry>
      <ID>32</ID>
      <DefaultText>[Descendre de la tour]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>33</ID>
      <DefaultText>[Utiliser un grappin pour descendre]</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>35</ID>
      <DefaultText>Vous descendez au pied de la tour.</DefaultText>
      <FemaleText />
    </Entry>
