      <ID>857</ID>
      <DefaultText>Langue de Xaurip</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>859</ID>
      <DefaultText>Cloche de Berath</DefaultText>
      <FemaleText />
    </Entry>
