      <ID>899</ID>
      <DefaultText>Sucre</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>901</ID>
      <DefaultText>Bière</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>936</ID>
      <DefaultText>Torche</DefaultText>
      <FemaleText />
    </Entry>
