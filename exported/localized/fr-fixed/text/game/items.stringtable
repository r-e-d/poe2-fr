      <ID>865</ID>
      <DefaultText>Berceau d'orlan</DefaultText>
      <FemaleText />
    </Entry>
    <Entry>
      <ID>867</ID>
      <DefaultText>Roseau de rivière</DefaultText>
      <FemaleText />
    </Entry>
